<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryColor" Color="#000000"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#DA0063"/>
    <SolidColorBrush x:Key="SecondaryColor_Highlight" Color="#E374A6"/>
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#E6E6E6"/>
    <SolidColorBrush x:Key="White" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Warning" Color="#FF0037"/>
    <SolidColorBrush x:Key="WarningHover" Color="#FF747C"/>


    <Color x:Key="Shadow" A="255" R="220" G="220" B="220"/>
    
</ResourceDictionary>