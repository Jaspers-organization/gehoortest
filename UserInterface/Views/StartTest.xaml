<UserControl x:Class="UserInterface.Views.StartTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:Views="clr-namespace:UserInterface.Views" 
        mc:Ignorable="d">
    <Grid>
        <Views:TestExplaination Visibility="{Binding IntroText}" />
        <Views:Question Visibility="{Binding QuestionText}" />
        <Views:AudiometryQuestion Visibility="{Binding QuestionAudio}" />
    </Grid>
</UserControl>
